 function reload(scheduleId){
      $.ajax({
        url: "/schedules/" + scheduleId,
        method: "GET",
        dataType: "HTML",

        error: function (xhr) {
          //$('#newpage').html("ERROR");
          alert("error");
        },
        success: function (data) {
          $("#schedule-review").html(data);
          alert("<%=@msgResult%>");


        }
      });
    }
    reload(<%=@detail.schedule.id%>);