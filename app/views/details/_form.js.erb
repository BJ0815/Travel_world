 function reload(scheduleId){
      $.ajax({
        url: "/schedules/" + scheduleId,
        method: "GET",
        dataType: "HTML",

        error: function (xhr) {
          alert("error");
        },
        success: function (data) {
          $("#places").html(data);
          initialize(<%= raw @schedule.spots.to_json %>)
          alert("<%=@msgResult%>");
          $("#detail .close").click();

        }
      });
    }
    reload(<%=@detail.schedule.id%>);
