 function reload(scheduleId){
      $.ajax({
        url: "/schedules/" + scheduleId,
        method: "GET",
        dataType: "HTML",

        error: function (xhr) {
          alert("error");
        },
        success: function (data) {
          $("#places").html(data);
          $("#newpage").html("<%=@msgResult%>");
          //有異動spot應該要刪除
          setMapOnAll();
          showSpot = false;

        }
      });
    }
    reload(<%=@detail.schedule.id%>);
