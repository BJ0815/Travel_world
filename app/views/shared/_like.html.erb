<% if liked?(object, current_user) %>
  <div id="event-id-<%= object.id %>" class="d-inline">
    <%= link_to (fa_icon "thumbs-up 2x"), unlike_event_path(object),remote: true, method: :post, class: "btn btn-default" %>
  </div>
<% else %>
  <div id="event-id-<%= object.id %>" class="d-inline">
    <%= link_to (fa_icon "thumbs-o-up 2x"), like_event_path(object),remote: true, method: :post, class: "btn btn-default" %>
  </div>
<% end %>